<view class='container'>
  <view class='user-info'>
    <image class='user-img' src='../../images/user.png'></image>
    <view></view>
  </view>
  <!-- <image class='user-img' src='../../images/user.png'></image> -->
  <view class='abouts'>
    <view class='my-task' bindtap='myJob'>
      <view class='task-img'>
        <image class='about-img' src='../../images/task.png' />
      </view>
      <view class='task-t'>
        <view class='my-task-1'>我的任务</view>
        <view class='my-task-2'>></view>
      </view>
    </view>
    <view class='problem' bindtap='problem'>
      <view class='problem-img'>
        <image class='about-img' src='../../images/problem.png' />
      </view>
       <view class='task-t'>
        <view class='my-task-1'>常见问题</view>
        <view class='my-task-2'>></view>
      </view>
    </view>
    <view class='feedback' bindtap='feedback'>
      <view class='feedback-img'>
        <image class='about-img' src='../../images/feedback.png'></image>
      </view>
       <view class='task-t'>
        <view class='my-task-1'>意见反馈</view>
        <view class='my-task-2'>></view>
      </view>
    </view>
    <view class='aboutUs' bindtap='aboutUs'>
      <view class='aboutUs-img'>
        <image class='about-img' src='../../images/aboutUs.png'></image>
      </view>
       <view class='task-t'>
        <view class='my-task-1'>关于我们</view>
        <view class='my-task-2'>></view>
      </view>
    </view>
  </view>
</view>